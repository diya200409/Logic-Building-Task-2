        "#Problem 10: Calculate Average Marks\n",
        "n = int(input(\"Enter number of subjects: \"))\n",
        "marks = []\n",
        "\n",
        "for i in range(n):\n",
        "    marks.append(float(input(\"Enter marks: \")))\n",
        "\n",
        "average_marks = sum(marks) / n\n",
        "\n",
        "print(\"Average Marks:\", average_marks)"
