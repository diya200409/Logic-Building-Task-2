        "#Problem 3: Find Maximum and Minimum Values\n",
        "n = int(input(\"Enter number of elements: \"))\n",
        "numbers = []\n",
        "\n",
        "for i in range(n):\n",
        "    numbers.append(int(input(\"Enter number: \")))\n",
        "\n",
        "maximum = numbers[0]\n",
        "minimum = numbers[0]\n",
        "\n",
        "for num in numbers:\n",
        "    if num > maximum:\n",
        "        maximum = num\n",
        "    if num < minimum:\n",
        "        minimum = num\n",
        "\n",
        "print(\"List:\", numbers)\n",
        "print(\"Max:\", maximum)\n",
        "print(\"Min:\", minimum)"
      ],
