        "#Problem 4: Count Products Above a Price Threshold\n",
        "n = int(input(\"Enter number of products: \"))\n",
        "prices = []\n",
        "\n",
        "for i in range(n):\n",
        "    prices.append(int(input(\"Enter product price: \")))\n",
        "\n",
        "count = 0\n",
        "\n",
        "for price in prices:\n",
        "    if price > 1000:\n",
        "        count += 1\n",
        "\n",
        "print(\"Products above 1000:\", count)"
