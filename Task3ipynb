        "#Problem 7: Count Character Frequency\n",
        "text = input(\"Enter a string: \")\n",
        "char_frequency = {}\n",
        "\n",
        "for char in text:\n",
        "    if char in char_frequency:\n",
        "        char_frequency[char] += 1\n",
        "    else:\n",
        "        char_frequency[char] = 1\n",
        "\n",
        "print(char_frequency)"
